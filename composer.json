{
  "name": "gboquizosanchez/phpstan-report",
  "description": "Laravel package to display PHPStan errors in a view",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Germán Boquizo Sánchez",
      "email": "germanboquizosanchez@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/http": "^11.0|^12.0",
    "illuminate/console": "^11.0|^12.0",
    "illuminate/view": "^11.0|^12.0",
    "phpstan/phpstan": "^1.0|^2.0"
  },
  "require-dev": {
    "laravel/pint": "^1.24"
  },
  "autoload": {
    "psr-4": {
      "Boquizo\\PhpstanReport\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Boquizo\\PhpstanReport\\PhpstanReportProvider"
      ]
    }
  }
}
